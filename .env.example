## Required
ASSEMBLYAI_API_KEY=

## Optional (defaults shown below)
OUTPUT_DIR=output
AUDIO_DIR=audio
FILENAME_STYLE=title_id          # id | id_title | title_id
AUDIO_FORMAT=mp3                # mp3 | wav
LANGUAGE_DETECTION=auto         # auto | manual
LANGUAGE_CODE=en_us             # AssemblyAI language_code
CONCURRENCY=2
MAX_VIDEOS=
AFTER_DATE=
CSV_ENABLED=false
COMMENTS_ENABLED=false
COMMENTS_MAX=

## AssemblyAI credits preflight
ASSEMBLYAI_CREDITS_CHECK=warn    # warn | abort | none
ASSEMBLYAI_MIN_BALANCE_MINUTES=60

## yt-dlp integration
YT_DLP_PATH=
YT_DLP_EXTRA_ARGS=[]             # e.g. ["--extractor-args","youtube:player_client=default"]
