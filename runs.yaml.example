# Copy to runs.yaml (ignored by git) and configure your channels.
# YAML uses spaces only, no tabs.

runs:
  - url: "https://www.youtube.com/@somechannel"
    maxNewVideos: 10
    after: "2024-01-01"
    concurrency: 2
    csvEnabled: false
    sttProvider: assemblyai
    openaiWhisperModel: whisper-1
    maxAudioMB:
    splitOverlapSeconds: 2
    filenameStyle: title_id
    languageDetection: auto
    languageCode: en_us
    assemblyAiCreditsCheck: warn
    assemblyAiMinBalanceMinutes: 60
    commentsEnabled: false
    commentsMax:
    ytDlpPath:

  - url: "https://www.youtube.com/playlist?list=PLxxxxxxxxxxxx"
    maxNewVideos: 5
    after: "2023-06-01"
    outDir: "output_custom"
    audioDir: "audio_custom"
    csvEnabled: true
    sttProvider: assemblyai
    openaiWhisperModel: whisper-1
    maxAudioMB:
    splitOverlapSeconds: 2
    filenameStyle: id_title
    languageDetection: manual
    languageCode: en_us
    assemblyAiCreditsCheck: warn
    assemblyAiMinBalanceMinutes: 60
    commentsEnabled: true
    commentsMax: 200
    force: false
    ytDlpPath:
